<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="windowNoTitle">true</item>

        <!-- color high light -->
        <item name="colorControlHighlight">#AECDFF</item>
    </style>
    <style name="myFrameLayout">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">0dp</item>
        <item name="layout_constraintWidth_default">percent</item>
        <item name="layout_constraintWidth_percent">0.13</item>
        <item name="layout_constraintDimensionRatio">1</item>
        <item name="layout_constraintHorizontal_chainStyle">packed</item>
        <item name="layout_constraintHorizontal_bias">0.5</item>
    </style>
    <style name = "myImageView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>
</resources>
